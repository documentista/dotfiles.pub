#!/bin/bash

cd

repo=.dotfiles.pub

for item in bin.pub .abcde.conf .weechat '.bash*' '.vim*' .tmux.conf .inputrc .emacs.d; do
    ln -vsf ~/$repo/$item ~/
done

ln -vsf ~/$repo/.gitconfig ~/
ln -vsf ~/$repo/.gitignore ~/

mkdir -p ~/.lein
ln -vsf ~/$repo/.lein/profiles.clj ~/.lein/

