[user]
	name = Tom Dolan
	email = tgdolan@gmail.com

[core]
	pager = less -FXRS

[color]
    branch = auto
    diff = auto
    status = auto

[alias]
    pr = pull --rebase
    st = status
    ci = commit
    log1 = log --pretty=oneline --decorate
    rbu = rebase @{upstream}

[merge]
	tool = vimdiff

